<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >
    <LinearLayout
        android:id="@+id/layout_p2p"
        android:layout_width="match_parent"
	    android:layout_height="match_parent"
	    android:layout_toLeftOf="@+id/p2p_control"
        >
        <com.p2p.core.P2PView
		    android:id="@+id/pView"
		    android:layout_width="match_parent"
		    android:layout_height="match_parent"
		    android:layout_centerInParent="true"
		    >
	    
		</com.p2p.core.P2PView>
    </LinearLayout>
     <RelativeLayout
        android:id="@+id/layout_telescopic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true" >

        <LinearLayout
            android:id="@+id/layout_gpio"
            android:layout_width="110dp"
            android:layout_height="120dp"
            android:layout_alignParentLeft="true"
            android:layout_centerInParent="true"
            android:background="@drawable/img1"
            android:orientation="vertical"
            android:visibility="gone" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_gravity="center_horizontal"
                android:layout_weight="1"
                android:orientation="horizontal" >

                <RelativeLayout
                    android:id="@+id/tv_on1"
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:background="@drawable/selector_gpio">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:background="@drawable/selector_gpio"
                        android:text="ON"
                        android:textColor="@color/text_color_white"
                        android:textSize="@dimen/text_size_small" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/selector_gpio" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:text="1"
                        android:textColor="@color/text_color_white"
                        android:textSize="@dimen/text_size_small" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/tv_off1"
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:background="@drawable/selector_gpio" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:text="OFF"
                        android:textColor="@color/text_color_white"
                        android:textSize="@dimen/text_size_small" />
                </RelativeLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_gravity="center_horizontal"
                android:layout_weight="1"
                android:orientation="horizontal" >

                <RelativeLayout
                    android:id="@+id/tv_on2"
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:background="@drawable/selector_gpio" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:text="ON"
                        android:textColor="@color/text_color_white"
                        android:textSize="@dimen/text_size_small" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/selector_gpio" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:text="2"
                        android:textColor="@color/text_color_white"
                        android:textSize="@dimen/text_size_small" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/tv_off2"
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:background="@drawable/selector_gpio" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:text="OFF"
                        android:textColor="@color/text_color_white"
                        android:textSize="@dimen/text_size_small" />
                </RelativeLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_gravity="center_horizontal"
                android:layout_weight="1"
                android:orientation="horizontal" >

                <RelativeLayout
                    android:id="@+id/tv_on3"
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:background="@drawable/selector_gpio" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:text="ON"
                        android:textColor="@color/text_color_white"
                        android:textSize="@dimen/text_size_small" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/selector_gpio" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:text="3"
                        android:textColor="@color/text_color_white"
                        android:textSize="@dimen/text_size_small" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/tv_off3"
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:background="@drawable/selector_gpio" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:text="OFF"
                        android:textColor="@color/text_color_white"
                        android:textSize="@dimen/text_size_small" />
                </RelativeLayout>
            </LinearLayout>
        </LinearLayout>

        <RelativeLayout
            android:id="@+id/layout_arrow"
            android:layout_width="30dp"
            android:layout_height="120dp"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/layout_gpio"
            android:background="@drawable/img2"
            android:visibility="gone">
            <ImageView
                android:id="@+id/img_arrow"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/b_right"
                android:layout_centerInParent="true"
               />
        </RelativeLayout>
    </RelativeLayout>
    <LinearLayout 
         android:id="@+id/control_top"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_above="@+id/control_bottom"
         android:layout_alignParentLeft="true"
         android:layout_marginLeft="@dimen/inner_margin_right"
         android:orientation="vertical"
         android:background="@drawable/control_bar_bg"
         android:visibility="gone"
         >
        <TextView 
            android:id="@+id/video_mode_hd"
            android:layout_width="@dimen/p2p_control_top_item_width"
            android:layout_height="@dimen/p2p_control_top_item_height"
            android:background="@drawable/selector_play_back"
            android:textColor="@color/text_color_white"
            android:textSize="@dimen/text_size_small"
            android:text="@string/video_mode_hd"
            android:gravity="center"
            />
        <View 
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/text_color_white"/>
         <TextView 
            android:id="@+id/video_mode_sd"
            android:layout_width="@dimen/p2p_control_top_item_width"
            android:layout_height="@dimen/p2p_control_top_item_height"
            android:background="@drawable/selector_play_back"
            android:textColor="@color/text_color_white"
            android:textSize="@dimen/text_size_small"
            android:text="@string/video_mode_sd"
            android:gravity="center"
            />
          <View 
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/text_color_white"/>
         <TextView 
            android:id="@+id/video_mode_ld"
            android:layout_width="@dimen/p2p_control_top_item_width"
            android:layout_height="@dimen/p2p_control_top_item_height"
            android:background="@drawable/selector_play_back"
            android:textColor="@color/text_color_blue"
            android:textSize="@dimen/text_size_small"
            android:text="@string/video_mode_ld"
            android:gravity="center"
            />
     </LinearLayout>
     
	<RelativeLayout 
         android:id="@+id/control_bottom"
         android:layout_width="match_parent"
         android:layout_height="@dimen/p2p_monitor_bar_height"
         android:layout_centerHorizontal="true"
         android:layout_alignParentBottom="true"
         android:background="@drawable/control_bar_bg"
         >
          <LinearLayout 
             android:layout_width="wrap_content"
             android:layout_height="match_parent"
             android:layout_centerInParent="true"
             android:orientation="horizontal"
             android:focusable="true"
             android:clickable="true"
             >
            <ImageView 
                 android:id="@+id/open_door"
                 android:layout_width="@dimen/monitor_btn_width"
                 android:layout_height="@dimen/monitor_btn_width"
                 android:layout_gravity="center_vertical"
                 android:background="@drawable/m_open_door"
                 android:scaleType="fitCenter"
                 android:visibility="gone"/>
            <ImageView
    	 	    android:id="@+id/defence_state"
    	 	    android:layout_width="@dimen/monitor_btn_width"
                android:layout_height="@dimen/monitor_btn_width"
    	 	    android:layout_marginLeft="15dp"
    	 	    android:scaleType="fitCenter"
    	 	    android:layout_gravity="center_vertical"
    	 	    android:background="@drawable/disarm" 
    	 	    android:visibility="gone"
    	 	    />
           
             <ImageView 
                 android:id="@+id/close_voice"
                 android:layout_width="@dimen/monitor_btn_width"
                 android:layout_height="@dimen/monitor_btn_width"
                 android:layout_marginLeft="15dp"
                 android:layout_gravity="center_vertical"
                 android:background="@drawable/m_voice_on"
                 android:scaleType="fitCenter"
                 />
             <ImageView
    	 	    android:id="@+id/send_voice"
    	 	    android:layout_width="@dimen/monitor_btn_width"
                android:layout_height="@dimen/monitor_btn_width"
    	 	    android:layout_marginLeft="15dp"
    	 	    android:layout_gravity="center_vertical"
    	 	    android:scaleType="fitCenter"
    	 	    android:background="@drawable/ic_send_audio"
    	 	    />
             <ImageView 
                 android:id="@+id/screenshot"
                 android:layout_width="@dimen/monitor_btn_width"
                 android:layout_height="@dimen/monitor_btn_width"
                 android:layout_marginLeft="15dp"
                 android:layout_gravity="center_vertical"
                 android:background="@drawable/m_screenshot"
                 android:scaleType="fitCenter"
                 />
		    <ImageView
    	 	    android:id="@+id/steering_wheel"
    	 	    android:layout_width="@dimen/monitor_btn_width"
                android:layout_height="@dimen/monitor_btn_width"
    	 	    android:layout_marginLeft="15dp"
    	 	    android:layout_gravity="center_vertical"
    	 	    android:scaleType="fitCenter"
    	 	    android:background="@drawable/direction_p" 
    	 	    android:visibility="gone"
    	 	    /> 
         </LinearLayout>
         <Button 
                 android:id="@+id/choose_video_format"
                 android:layout_width="@dimen/p2p_control_top_item_width"
                 android:layout_height="@dimen/video_format_height"
                 android:layout_alignParentLeft="true"
                 android:layout_centerVertical="true"
                 android:layout_marginLeft="@dimen/inner_margin_right"
                 android:text="@string/video_mode_sd"
                 android:textColor="@color/text_color_white"
                 android:textSize="@dimen/text_size_small"
                 android:background="@drawable/m_sd_background"
                />
         <ImageView
    	 	    android:id="@+id/hungup"
    	 	    android:layout_width="@dimen/monitor_btn_width"
                android:layout_height="@dimen/monitor_btn_width"
    	 	    android:layout_alignParentRight="true"
    	 	    android:layout_centerVertical="true"
    	 	    android:layout_marginRight="20dp"	  
    	 	    android:background="@drawable/m_hangup"
    	 	    />
        </RelativeLayout>
    
     <RelativeLayout
         android:id="@+id/layout_steering_wheel" 
         android:layout_width="100dp"
         android:layout_height="100dp"
         android:layout_toRightOf="@+id/control_bottom"
         android:layout_alignParentBottom="true"
         android:layout_marginLeft="10dp"
         android:visibility="gone"
         >
         <Button 
	        android:id="@+id/top_btn"
	        android:layout_width="35dp"
	        android:layout_height="35dp"
	        android:layout_alignParentTop="true"
	        android:layout_centerHorizontal="true"
	        android:background="@drawable/top"
	        />
        <Button 
	        android:id="@+id/bottom_btn"
	        android:layout_width="35dp"
	        android:layout_height="35dp"
	        android:layout_alignParentBottom="true"
	        android:layout_centerHorizontal="true"
	        android:background="@drawable/bottom"
	        />
        <Button 
	        android:id="@+id/left_btn"
	        android:layout_width="35dp"
	        android:layout_height="35dp"
	        android:layout_alignParentLeft="true"
	        android:layout_centerVertical="true"
	        android:background="@drawable/left"
	        />
        <Button 
	        android:id="@+id/right_btn"
	        android:layout_width="35dp"
	        android:layout_height="35dp"
	        android:layout_alignParentRight="true"
	        android:layout_centerVertical="true"
	        android:background="@drawable/right"
	        />
     </RelativeLayout>
	 <LinearLayout 
	     android:visibility="gone"
	     android:id="@+id/layout_voice_state"
	     android:layout_width="@dimen/p2p_monitor_voice_state_width_and_height"
	     android:layout_height="@dimen/p2p_monitor_voice_state_width_and_height"
	     android:layout_alignParentLeft="true"
	     android:layout_above="@+id/control_bottom"
	     android:orientation="horizontal"
	     android:layout_marginLeft="5dp"
	     android:layout_marginBottom="5dp"
	     android:background="@color/alpha"
	     >
         
	     <ImageView 
	         android:layout_width="0dp"
	         android:layout_weight="1"
	         android:layout_height="60dp"
	         android:src="@drawable/voice"
	         android:scaleType="fitCenter"
	         android:layout_gravity="center"
	         />
	     <ImageView 
	         android:id="@+id/voice_state"
	         android:layout_width="0dp"
	         android:layout_weight="1"
	         android:layout_height="match_parent"
	         android:src="@anim/voice_state"
	         android:scaleType="fitCenter"
	         />
	     
	 </LinearLayout>    
     <RelativeLayout 
	     android:layout_width="wrap_content"
	     android:layout_height="wrap_content"
	     android:layout_alignParentRight="true"
	     android:background="@drawable/monitor_number"
	     >
	 </RelativeLayout>   
	 <TextView 
            android:id="@+id/text_number"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_alignParentRight="true"
            android:textColor="@color/text_color_white"
            android:textSize="@dimen/text_size_normal"
            android:text="当前观看人数：1"   
     />
     
</RelativeLayout>